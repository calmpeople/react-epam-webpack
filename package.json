{
  "name": "react-webpak",
  "version": "1.0.0",
  "description": "Boilerplate for React apps",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:prod": "cross-env NODE_ENV=production node_modules/.bin/webpack-dev-server -p",
    "start:dev": "cross-env NODE_ENV=development node_modules/.bin/webpack-dev-server --open --hot",
    "build:prod": "cross-env NODE_ENV=production node_modules/.bin/webpack -p",
    "build:dev": "cross-env NODE_ENV=development node_modules/.bin/webpack",
    "code:check": "./node_modules/.bin/eslint '**/*.js'",
    "code:fix": "./node_modules/.bin/eslint '**/*.js' --fix"
  },
  "author": "Siarhei",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
